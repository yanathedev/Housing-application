iv class=\"input-group\">\n                        <label for=\"price\">Price*</label>\n                        <input type=\"text\" v-model=\"formData.price\" name=\"price\" id=\"price\" placeholder=\"e.g. â‚¬150.000\" />\n                    </div>\n        \n                    <div class=\"display-connected\">\n                        <div class=\"input-group\">\n                            <label for=\"size\">Size*</label>\n                            <input type=\"text\" v-model=\"formData.size\" name=\"size\" id=\"size\" placeholder=\"e.g. 60m2\" />\n                        </div>\n        \n                        <div class=\"input-group\">\n                            <label id=\"garage\">Garage*</label>\n                            <select name=\"garage\" v-model=\"formData.hasGarage\" id=\"garage\" required>\n                                <option value=\"select\">Select</option>\n                                <option value=\"option1\">Option-1</option>\n                                <option value=\"option2\">Option-2</option>\n                            </select>\n                        </div>\n                    </div>\n        \n                    <div class=\"display-connected\">\n                        <div class=\"input-group\">\n                            <label for=\"bedrooms\">Bedrooms*</label>\n                            <input type=\"text\" v-model=\"formData.bedrooms\" name=\"bedrooms\" id=\"bedrooms\" placeholder=\"Enter amount\" />\n                        </div>\n        \n                        <div class=\"input-group\">\n                            <label for=\"bathrooms\" id=\"garage\">Bathrooms*</label>\n                            <input type=\"text\" v-model=\"formData.bathrooms\" name=\"bathrooms\" id=\"bathrooms\" placeholder=\"Enter amount\" />\n                        </div>\n                    </div>\n        \n                    <div class=\"input-group\">\n                        <label for=\"construction\">Construction date*</label>\n                        <input type=\"text\" v-model=\"formData.constructionYear\" name=\"construction\" id=\"construction\" placeholder=\"e.g.1990\" />\n                    </div>\n        \n                    <div class=\"input-group\">\n                        <label for=\"description\">Description*</label>\n                        <input type=\"text\" v-model=\"formData.description\" id=\"description\">\n                    </div>\n        \n                    <div class=\"input-group full\">\n                        <button @click.prevent=\"addProperty\" type=\"submit\" class=\"submit\">SAVE</button>\n                        <div class=\"clear\"></div>\n                    </div>\n        \n                    <div class=\"errors\">\n        \n                    </div>\n            </form>\n        </main>\n        </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n    data() {\n        return {\n            formData:{\n                streetName: \"\",\n                houseNumber: \"\",\n                numberAddition: \"\",\n                zip: \"\",\n                city: \"\",\n                price: \"\",\n                size: \"\",\n                hasGarage: \"\",\n                bedrooms: \"\",\n                bathrooms: \"\",\n                constructionYear: \"\",\n                description: \"\"\n            }   \n        }\n    },\n\n    methods:{\n        addProperty(){\n            axios.post(\"https://api.intern.d-tt.nl/api/houses\",this.formData)\n            .then(response=>{\n                console.log(response)\n            })\n        }\n    }\n}\n\n</script>\n\n<style lang=\"scss\" scoped>\n    @import \"@/styles/listing.scss\";\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}